#. [message]: speaker=unit
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:421
msgid "Really? How comforting! Of course yes!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:466
msgid "Give us your heart, SON OF A LICH!!"
msgstr ""

#. [message]: speaker=crows
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:470
msgid "Noooooo!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:474
msgid "I'm not gonna let you!!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:498
msgid "Muhahahahahahahaha!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:502
msgid "Damn it!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:513
msgid "Heck!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:517
msgid "Vagabone! Close the rear!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:525
msgid "Do you show your back to the enemy? Shame on you!!"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:529
msgid "Humph!! I will tear through this cave!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/14_bridge.cfg:103
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:533
#: Danse_Macabre/scenarios/14_night_of_the_living_dead.cfg:1560
msgid "What!?"
msgstr ""

#. [message]: speaker=Mal Oryzias
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:537
msgid "Have fun!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/15_dawn_of_the_dead.cfg:409
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:541
msgid "Wait!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:559
msgid "Jesus!!"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:563
msgid "Hey you, Vagabone! Why do you obey that bastard!? He left you to die!!"
msgstr ""

#. [message]: speaker=bakabone
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:567
msgid "Meh"
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:571
msgid ""
"...OK, you are the paragon of undead!! Tear apart him!! Hurry up! We must "
"break away and chase after the lich!!"
msgstr ""

#. [objective]: condition=win
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:580
msgid "Macabre must reach the signpost."
msgstr ""

#. [message]: speaker=Macabre
#: Danse_Macabre/scenarios/12_the_magic_mountain.cfg:622
msgid "All right! Let's chase after the lich!"
msgstr ""

